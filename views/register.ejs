    <div class="container mt-5">
        <div class="row justify-content-center">
            <div class="col-md-6">
                <form action="/register" method="post">
                    <div class="form-group mb-4">
                        <label for="username">Name</label>
                        <input type="text" class="form-control" id="username" placeholder="Enter Name" name="contactname">
                    </div>
                    <div class="form-group mb-4">
                        <label for="contact">Number</label>
                        <input type="text" class="form-control" id="contact" placeholder="Enter Number" name="contact">
                    </div>
                    <div class="form-group mb-4">
                        <label for="email">Email</label>
                        <input type="email" class="form-control" id="email" placeholder="Enter Email" name="contactemail">
                    </div>
                    <div class="form-group mb-4">
                        <label for="address">Address</label>
                        <input type="text" class="form-control" id="address" placeholder="Enter Address" name="address">
                    </div>
                    <button type="submit" class="btn btn-primary btn-block">Add Contact</button>
                </form>
            </div>
        </div>
    </div>


